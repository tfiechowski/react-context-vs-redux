// jsconfig.json
{
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "components/*": ["src/components/*", "src/components/*/index.js"],
      "context": ["src/context/*", "src/context/*/index.js"],
      "redux": ["src/redux/*", "src/redux/*/index.js"],
    }
  },
  "include": ["src/**/*"]
}
